{
  "routes": [
    {
      "route": "/smile-spa/*",
      "allowedRoles": ["APP_ROLE_SWA_SMILESPA"]
    },
    {
      "route": "/debug/token-debug.html",
      "allowedRoles": ["authenticated"]
    }
  ],
  "responseOverrides": {
    "401": { "statusCode": 302, "redirect": "/.auth/login/aad" },
    "403": { "statusCode": 302, "redirect": "/forbidden.html" }
  },
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/assets/*",
      "/images/*",
      "/.auth/*",
      "/equipment/*",
      "/forbidden.html",
      "/logout-complete/*",
      "/debug/*"
    ]
  }
}
