{
  "routes": [
    {
      "route": "/api/{*rest}",
      "allowedRoles": [
        "anonymous"
      ],
      "rewrite": "https://practx-prod-apim.azure-api.net/practx/{rest}"
    },
    {
      "route": "/practice/*",
      "allowedRoles": [
        "anonymous"
      ]
    },
    {
      "route": "/patient/*",
      "allowedRoles": [
        "anonymous"
      ]
    },
    {
      "route": "/equipment/*",
      "allowedRoles": [
        "anonymous"
      ]
    },
    {
      "route": "/service/*",
      "allowedRoles": [
        "anonymous"
      ]
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/api/*",
      "/assets/*",
      "/robots.txt",
      "/sitemap.xml"
    ]
  }
}
